    "@material/mwc-dialog": "^0.20.0",
    "@material/mwc-textfield": "^0.20.0",
    "@polymer/iron-icons": "^3.0.1",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "anylogger": "^1.0.11",
    "bulma": "^0.8.2",
    "date-fns": "^2.19.0",
    "immutable": "^4.0.0-rc.14",
    "ramda": "latest",
    "sirv-cli": "^1.0.0",
    "stylelint-scss": "^3.19.0",
    "svelte-awesome": "^2.3.1",
    "svelte-navigator": "^3.1.5",
    "ulog": "^2.0.0-beta.18",
    "yargs": "^17.0.1"
